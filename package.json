{
  "name": "luxury-perfume-store",
  "version": "1.0.0",
  "description": "Luxury Perfume E-commerce Store with Vue 3 & Firebase",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:storage": "firebase deploy --only storage",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "generate-sitemap": "node sitemap-generator.js",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "firebase": "^10.14.1",
    "pinia": "^2.1.0",
    "uuid": "^9.0.0",
    "vue": "^3.4.0",
    "vue-i18n": "^12.0.0-alpha.3",
    "vue-router": "^4.2.0",
    "vuefire": "^3.2.2",
    "yup": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.5",
    "vite": "^5.0.0",
    "vue-tsc": "^2.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "vue3",
    "firebase",
    "ecommerce",
    "luxury",
    "perfume",
    "bilingual",
    "typescript"
  ],
  "author": "Luxury Perfume Store",
  "license": "MIT"
}
